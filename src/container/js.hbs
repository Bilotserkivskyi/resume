<div>
    <main>
       <div>
        <h1>{{name}}</h1>        
        <hr>
        <q>{{description}}</q>
        <br><br>
            <div>
                <dl>
                    <dt>{{#with history}}</dt>
                    <dd>Year: {{year}}</dd>
                    <dd>Founder: {{founder}}</dd>
                        {{/with}}
                </dl>
            </div>
            <hr>

            <div>
                <h2>Types:</h2>
                <ul>
                    {{#each types}}
                    <li>{{this}}</li>
                    {{/each}}
                </ul>
            </div>
            <hr>

            <div>
                <h2>Syntax:</h2>
                <ol>
                    {{#with syntax}}
                    <li><h3>Variables:</h3></li>                    
                        <ul>
                            {{#each variables}}
                            <li>{{this}}</li>
                            {{/each}}                          
                        </ul>
                        <br>
                    <li><h3>Functions:</h3></li>                   
                        <ul>
                            {{#each functions}}
                            <li>{{this}}</li>
                            {{/each}}                          
                        </ul>
                        <br>
                    
                    <li><h3>Conditionals:</h3></li>                    
                        <ul>
                            {{#each conditionals}}
                            <li>{{this}}</li>
                            {{/each}}                          
                        </ul>
                        <br>
                    
                    <li><h3>Loops:</h3></li>                   
                        <ul>
                            {{#each loops}}
                            <li>{{this}}</li>
                            {{/each}}                          
                        </ul>
                        <br>
                    
                    <li><h3>Classes:</h3></li>                   
                        <ul>
                            {{#each classes}}
                            <li>{{this}}</li>
                            {{/each}}                          
                        </ul>
                        <br>

                    {{/with}}
                </ol>
            </div>
            <hr>

            <div>
                <h2>Features:</h2>
                <ul>                
                {{#each features}}
                <li>{{this}}</li>
                {{/each}}
                </ul>
            </div>
            <hr>

            <div>
                 <h2>Libraries:</h2>
                  <ul>                
                {{#each libraries}}
                <li>{{this}}</li>
                {{/each}}
                </ul>                
            </div>
            <hr>

             <div>
                 <h2>Tools:</h2>
                  <ul>                
                {{#each tools}}
                <li>{{this}}</li>
                {{/each}}
                </ul>                
            </div>
            <hr>

            <div>
                <h2>Community:</h2>
                {{#each community}}
                <dl>
                    <dt><b>{{name}}</b></dt>
                    <dd>{{type}}</dd>
                </dl>               
                {{/each}}
            </div>            
       </div>
    </main>
</div>