<div>
    <main>
        <div>
            <header>
                {{#with page_header}}
            <h1 class="text-center">{{title}}</h1>                
                <p class="text-uppercase text-center">
                    {{description}}
                </p >
                {{/with}}
            </header>

            <div>
                {{#with section}}
                    <div class="card card-body">
                        {{#with about_us}}
                            <div>
                                {{#with title_list}}
                                <div>
                                    <div class="display-6 text-end">{{main}}</div>
                                    <br>
                                    <h2 class="text-end">{{sub}}</h2>
                                </div>
                                {{/with}}
                            </div>

                            <div>
                                {{#with button}}
                                    <button class="btn btn-outline-info">{{text}}</button>
                                {{/with}}
                            </div>
                        {{/with}}
                    </div>
                    <br>
                    <br>

                    <div class="card card-body">
                        {{#with advantage}}
                        <span class="display-5 text-muted">{{title}}</span>
                        <br>
                            <div class="card card-body">
                                {{#with efficiency}}
                                <span class="display-4 text-muted">{{title}}</span><br>
                                <h6>{{sub_title}}</h6><br>
                                <p>
                                    {{description}}
                                    <br>
                                    <br>
                                    <button class="btn btn-outline-warning">{{button}}</button>
                                </p>
                                {{/with}}
                            </div>
                            <br>

                             <div class="card card-body">
                                {{#with innovation}}
                                <span class="display-4 text-center text-muted">{{title}}</span><br>
                                <h6 class="text-center text-muted">{{sub_title}}</h6>
                                <p>
                                    <p class="text-center text-muted">{{description}}</p>
                                    <br>
                                    
                                    <span><button class="btn btn-outline-warning">{{button}}</button></span>
                                </p>
                                {{/with}}
                            </div>
                            <br>

                            <div class="card card-body">
                                {{#with collaboration}}
                                <span class="display-4 text-muted text-end">{{title}}</span><br>
                                <h6 class="text-muted text-end">{{sub_title}}</h6><br>
                                <p>
                                    <div class="text-muted text-end">{{description}}</div>
                                    <br>
                                    <span><button class="btn btn-outline-warning">{{button}}</button></span>
                                </p>
                                {{/with}}
                            </div>
                            <br>
                        {{/with}}
                    </div>
                    <br>
                    <br>

                    <div class="card card-body">
                        {{#with agreement}}
                            <span class="display-5">{{title}}</span>
                            <br>
                            <div>
                                {{#with info}}
                                    <div class="card card-body">
                                        <h5>{{sub_title}}</h5>
                                        <div class="card card-body">
                                            {{#with detail}}
                                                <span>{{text}}</span><br>
                                                <span><button class="btn btn-success">{{button}}</button></span>
                                            {{/with}}
                                        </div>
                                    </div>
                                {{/with}}
                            </div>
                        {{/with}}                        
                    </div>
                {{/with}}                        
            </div>           

        </div>
    </main>
</div>