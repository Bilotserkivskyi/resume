<div>
    <main>
       <div>
            <div class="card card-body">
                {{#with heading}}
                <div class="card card-body">
                    <h1>H1 Heading</h1>
                    <p>
                    <span>Paragraph</span>
                    <br><br>
                    <button class="btn btn-primary">Primary</button>
                    <br><br>
                    <button class="btn btn-outline-primary">Outline Primary</button>
                    </p>
                    {{#with main}}
                    <div class="display-6">{{title}}</div>
                    <br>
                    <div>{{text}}</div>
                    <br>
                    <div>                        
                        {{#with button_text}}
                            <button class="btn btn-warning">{{solid}}</button>
                            <button class="btn btn-success">{{outline}}</button>
                        {{/with}}
                    </div>
                    <br>                    
                    {{/with}}
                </div>
                <br>

                <div class="card card-body">
                    <h2>H2 Heading second</h2>
                    <p>
                    <span class="text-muted text-capitalize">Paragraph</span>
                    </p>
                    
                    <p>
                    <button class="btn btn-secondary">Secondary</button>
                    <br><br>
                    <button class="btn btn-outline-secondary">Secondary</button>
                    </p>
                    {{#with second}}
                    <span class="display-6">{{title}}</span><br>
                    <span>{{text}}</span><br>
                        <div>
                            {{#with button_text}}
                                <button class="btn btn-warning">{{solid}}</button>
                                <button class="btn btn-success">{{outline}}</button>
                            {{/with}}
                        </div>
                    {{/with}}
                </div>
                <br>

                <div class="card card-body">
                    <h3>H3 Heading subblock</h3>
                    <p>
                    <span>Paragraph</span>
                    </p>
                    <p>
                    <button class="btn btn-dark">Dark</button>
                    <br><br>
                    <button class="btn btn-outline-dark">Dark</button>
                    </p>
                    {{#with subblock}}
                    <span class="display-6">{{title}}</span><br>
                    <span>{{text}}</span><br>
                        <div>
                            {{#with button_text}}
                                <button class="btn btn-warning">{{solid}}</button>                                
                                <button class="btn btn-success">{{outline}}</button>
                            {{/with}}
                        </div>
                    {{/with}}
                </div>
                <br>
                {{/with}}
            </div>
            <br>
            <br>

            <div class="card card-body">                
                {{#with nested_display}}
                <div class="display-1"><abbr>{{title}}</abbr></div>
                <br>
                <div class="card card-body">
                    {{#with children_display}}
                    <span class="display-4">{{title}}</span><br>
                    <span>{{text}}</span><br>
                    <div>
                        {{#with button}}
                        <button class="btn btn-outline-warning">{{text}}</button>
                        {{/with}}
                    </div>
                    {{/with}}
                </div>
                <br>

                <div class="display-6">
                    <p>{{header_text}}</p>                   
                </div>

                <div>
                    {{#with button}}
                        <button class="btn btn-info">{{text}}</button>
                    {{/with}}
                </div>
                {{/with}}
            </div>
            <br>
            <br>

            <div class="card card-body">
                {{#with paragraph}}
                <p class="text-uppercase">{{title}}</p><br>
                <p class="text-lowercase">{{text}}</p>
                    <div>
                        {{#with button}}
                       <button class="btn btn-link" href="{{url}}" target="_blank">{{text}}</button>
                        {{/with}}
                    </div>
                    <br>

                    <div class="card card-body">
                        <ul>
                        {{#each list_name}}
                        <li><a class='btn btn-link' href="{{url}}" target="_blank">{{text}}</a></li>
                        <br>
                        {{/each}}
                        </ul>                        
                    </div>
                {{/with}}
            </div>
        
       </div>
    </main>
</div>